<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_rs_student" model="ir.sequence">
        <field name="name">Student Account Number</field>
        <field name="code">rs_student</field>
        <field name="prefix">SCOM%(year)s%(month)s%(day)s</field>
        <field name="padding">5</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_rs_teacher" model="ir.sequence">
        <field name="name">Teacher Account Number</field>
        <field name="code">rs_teacher</field>
        <field name="prefix">PRF%(year)s</field>
        <field name="padding">5</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
